Buttons:false, cellAlign:'left', bgLazyLoad:3, accessibility:true, contain:1, initialIndex:1, imagesLoaded:true, freeScroll:true});function toggle(){if(flkty.selectedIndex>0){flkty.select(0);}else{flkty.select(1);}};function flick(id){for(i=0;i < flkty.cells.length; i++){if(flkty.cells[i]['element']['id'] == id){flkty.select(i);break;};};};function scroll(){var id=window.location.search.substr(1).split('=')[0];if(id){flick(id);};};</script></body></html>
